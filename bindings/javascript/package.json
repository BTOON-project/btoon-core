{
  "name": "btoon",
  "version": "0.0.1",
  "description": "High-performance binary serialization format for JavaScript/TypeScript",
  "main": "btoon.js",
  "types": "btoon.d.ts",
  "scripts": {
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha test/node/*.test.js",
    "test:browser": "karma start",
    "build": "npm run build:native && npm run build:bundle",
    "build:native": "node-gyp rebuild",
    "build:bundle": "webpack --mode production",
    "prebuild-install": "prebuild-install || npm run build:native",
    "install": "npm run prebuild-install",
    "benchmark": "node benchmark/index.js",
    "lint": "eslint btoon.js test/",
    "format": "prettier --write btoon.js btoon.d.ts test/",
    "prepare": "npm run build",
    "docs": "typedoc --out docs btoon.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btoon/btoon-core.git"
  },
  "keywords": [
    "serialization",
    "binary",
    "msgpack",
    "btoon",
    "compression",
    "schema",
    "fast",
    "typescript",
    "encoding",
    "decoding"
  ],
  "author": "BTOON Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btoon/btoon-core/issues"
  },
  "homepage": "https://github.com/btoon/btoon-core#readme",
  "files": [
    "btoon.js",
    "btoon.d.ts",
    "lib/",
    "build/",
    "binding.gyp",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.0.0",
    "karma": "^6.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "mocha": "^10.0.0",
    "node-gyp": "^9.0.0",
    "prebuild": "^11.0.0",
    "prebuild-install": "^7.0.0",
    "prettier": "^2.7.0",
    "typedoc": "^0.23.0",
    "typescript": "^4.8.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "optionalDependencies": {
    "pako": "^2.0.4"
  },
  "browser": {
    "./lib/native.js": false
  },
  "types": "btoon.d.ts",
  "typesVersions": {
    ">=4.0": {
      "*": [
        "btoon.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./btoon.d.ts",
      "require": "./btoon.js",
      "import": "./btoon.mjs",
      "browser": "./dist/btoon.min.js"
    },
    "./package.json": "./package.json"
  },
  "gypfile": true,
  "binary": {
    "module_name": "btoon_native",
    "module_path": "./build/{node_abi}-{platform}-{arch}/",
    "remote_path": "v{version}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/btoon/btoon-core/releases/download"
  }
}
